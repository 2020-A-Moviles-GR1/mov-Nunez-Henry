<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_dark"
    android:backgroundTint="@android:color/holo_blue_dark"
    tools:context=".DoctorActivity">

    <Button
        android:id="@+id/btn_pokemones"
        android:layout_width="104dp"
        android:layout_height="48dp"
        android:layout_marginTop="36dp"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:backgroundTint="@android:color/darker_gray"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="Ver Pacientes"
        android:textColor="#FAF8F8"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/btn_entre_a_main"
        android:layout_width="178dp"
        android:layout_height="39dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/darker_gray"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="Menu Principal"
        android:textColor="#FBFBFB"
        app:layout_constraintEnd_toEndOf="@+id/btn_modificar"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/btn_modificar"
        app:layout_constraintTop_toBottomOf="@+id/btn_modificar" />

    <TextView
        android:id="@+id/tv_pokemones"
        android:layout_width="147dp"
        android:layout_height="18dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:text="Any"
        android:textColor="@android:color/tertiary_text_light"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/tv_activo_entre" />

    <TextView
        android:id="@+id/tv_activo_entre"
        android:layout_width="147dp"
        android:layout_height="18dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="16dp"
        android:text="Any"
        android:textColor="@android:color/tertiary_text_light"
        app:layout_constraintStart_toEndOf="@+id/tv_j"
        app:layout_constraintTop_toBottomOf="@+id/tv_nivel" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="16dp"
        android:text="Pacientes"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintEnd_toEndOf="@+id/tv_j"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_j" />

    <TextView
        android:id="@+id/tv_j"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="16dp"
        android:text="Peso"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/tv_nivel"
        android:layout_width="147dp"
        android:layout_height="18dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:text="Any"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/tv_color" />

    <TextView
        android:id="@+id/tv_color"
        android:layout_width="147dp"
        android:layout_height="18dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="20dp"
        android:text="Any"
        android:textColor="@android:color/tertiary_text_light"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombre" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:text="edad"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:text="tipo de sangre"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:text="nombre"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="376dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:background="#FFFFFF"
        android:backgroundTint="@android:color/background_light"
        android:text="Doctor"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nombre"
        android:layout_width="147dp"
        android:layout_height="18dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="20dp"
        android:text="Any"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btn_modificar"
        android:layout_width="103dp"
        android:layout_height="48dp"
        android:layout_marginStart="12dp"
        android:background="#3F51B5"
        android:backgroundTint="@android:color/tertiary_text_light"
        android:lineSpacingExtra="30sp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="Modificar"
        android:textColor="#F8F8F8"
        app:layout_constraintBottom_toBottomOf="@+id/btn_pokemones"
        app:layout_constraintStart_toEndOf="@+id/btn_pokemones"
        app:layout_constraintTop_toTopOf="@+id/btn_pokemones"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn_eliminar"
        android:layout_width="98dp"
        android:layout_height="43dp"
        android:layout_marginStart="36dp"
        android:background="#3F51B5"
        android:backgroundTint="@android:color/tertiary_text_light"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="Eliminar"
        android:textColor="#FAF8F8"
        app:layout_constraintBottom_toBottomOf="@+id/btn_modificar"
        app:layout_constraintStart_toEndOf="@+id/btn_modificar"
        app:layout_constraintTop_toTopOf="@+id/btn_modificar"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>